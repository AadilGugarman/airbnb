<% layout('layouts/boilerplate') %>
<body>
  <div class="container my-3"></div>
  <div class="row ms-2">
    <% for (let listing of allListings ){ %>
    <div class="card mb-2 col-lg-3" style="width: 20rem">
      <img src="<%=listing.image%>" class="card-img-top" alt="listing_image" />
      <div class="card-body">
        <p class="card-text"><%=listing.title%></p>
        <p class="card-text"><%=listing.price.toLocaleString('EN-in')%></p>
      </div>
    </div>
    <%}%>
  </div>
</body>
